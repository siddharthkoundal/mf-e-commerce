#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/micro-fes-beginner-to-expert/home/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/bin/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/micro-fes-beginner-to-expert/home/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/micro-fes-beginner-to-expert/home/node_modules/.pnpm/sshpk@1.17.0/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/micro-fes-beginner-to-expert/home/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/micro-fes-beginner-to-expert/home/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/bin/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/micro-fes-beginner-to-expert/home/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/micro-fes-beginner-to-expert/home/node_modules/.pnpm/sshpk@1.17.0/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/micro-fes-beginner-to-expert/home/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sshpk@1.17.0/node_modules/sshpk/bin/sshpk-verify" "$@"
else
  exec node  "$basedir/../../../../../sshpk@1.17.0/node_modules/sshpk/bin/sshpk-verify" "$@"
fi
