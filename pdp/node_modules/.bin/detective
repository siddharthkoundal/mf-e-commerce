#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/project-1/pdp/node_modules/detective/bin/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/project-1/pdp/node_modules/detective/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/project-1/pdp/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/project-1/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/node_modules:/Users/siddharthkoundal/Downloads/node_modules:/Users/siddharthkoundal/node_modules:/Users/node_modules:/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/project-1/pdp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/project-1/pdp/node_modules/detective/bin/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/project-1/pdp/node_modules/detective/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/project-1/pdp/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/project-1/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/node_modules:/Users/siddharthkoundal/Downloads/node_modules:/Users/siddharthkoundal/node_modules:/Users/node_modules:/node_modules:/Users/siddharthkoundal/Downloads/codeFiles/webDev/micro-frontend/project-1/pdp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../detective/bin/detective.js" "$@"
else
  exec node  "$basedir/../detective/bin/detective.js" "$@"
fi
